javac -classpath /usr/local/hadoop-2.7.2/share/hadoop/common/hadoop-common-2.7.2.jar:/usr/local/hadoop-2.7.2/share/hadoop/mapreduce/hadoop-mapreduce-client-core-2.7.2.jar:/usr/local/hadoop-2.7.2/share/hadoop/common/lib/commons-cli-1.2.jar:/usr/local/hadoop-2.7.2/share/hadoop/common/lib/commons-logging-1.1.3.jar:./ Join.java

jar -cvf Join.jar Join*.class

gcc -Wall -fPIC -c to_gpu.c -I./ -I/usr/local/jdk1.8.0_101/include -I/usr/local/jdk1.8.0_101/include/linux

gcc -Wall -rdynamic -shared -o libjnidll.so to_gpu.o

bin/hadoop jar /home/dblab/lijianing/lijianing_join/lijianing_theta/Join.jar Join


hdfs dfs -rm -R /tmp/output
